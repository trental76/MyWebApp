@using MudBlazor
@using MyWebApp.Models

<MudGrid Gutter="3">
    @foreach (var item in Items)
    {
        <MudItem xs="12" sm="6" md="4" lg="3">
            <ItemCard Item="item" />
        </MudItem>
    }
</MudGrid>

@code {
    [Parameter] public List<SectionItem> Items { get; set; } = new();
}